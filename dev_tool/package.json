{
    "name": "dev_tool",
    "type": "module",
    "version": "1.0.0",
    "description": "local dev tooling",
    "main": "index.js",
    "private": true,
    "license": "MIT",
    "scripts": {
        "precommit": "lint-staged",
        "lint": "tsc --noEmit && eslint --max-warnings=0",
        "build": "tsc",
        "build:dev": "tsc",
        "prettier": "npx prettier -u --write \"**/*.+(ts|tsx|json)\""
    },
    "lint-staged": {
        "**/*.{js,ts}": [
            "eslint --fix --max-warnings=0",
            "prettier --write src/**/*.ts{,x}"
        ]
    },
    "dependencies": {
        "@aws-sdk/client-cloudfront": "^3.895.0",
        "@aws-sdk/client-ec2": "^3.895.0",
        "@aws-sdk/client-secrets-manager": "^3.895.0",
        "axios": "^1.12.1",
        "node-ssh": "^13.2.1",
        "prompts": "^2.4.2",
        "yargs": "^18.0.0"
    },
    "devDependencies": {
        "@types/eslint": "^9.6.1",
        "@types/node": "^24.2.1",
        "@types/prompts": "^2.4.9",
        "@types/ssh2": "^1.11.15",
        "@types/yargs": "^17.0.33",
        "@typescript-eslint/eslint-plugin": "^8.39.1",
        "@typescript-eslint/parser": "^8.29.1",
        "eslint": "^9.34.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "jiti": "^2.0.0",
        "lint-staged": "^15.2.7",
        "prettier": "^3.5.3",
        "typescript": "5.9.2"
    }
}
