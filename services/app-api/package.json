{
    "name": "app-api",
    "private": "true",
    "version": "1.0.0",
    "main": "handler.js",
    "license": "MIT",
    "scripts": {
        "precommit": "lint-staged",
        "test": "vitest --watch",
        "test:once": "vitest --runInBand --ci",
        "test:coverage": "vitest --coverage",
        "lint": "tsc --noEmit && eslint . --max-warnings=0",
        "generate": "pnpm prisma generate",
        "prisma:reset": "pnpm prisma migrate reset --force",
        "build": "tsc --noEmit",
        "start": "pnpm serverless offline start --stage local --region us-east-1 --httpPort 3030 --host 127.0.0.1"
    },
    "lint-staged": {
        "*.{js,ts}": [
            "eslint --max-warnings=0 --ignore-pattern 'esbuild.config.js'",
            "prettier --write"
        ],
        "*.sql": [
            "sh ../../scripts/validate_migration.sh"
        ]
    },
    "dependencies": {
        "@mc-review/common-code": "workspace:*",
        "@mc-review/constants": "workspace:*",
        "@mc-review/dates": "workspace:*",
        "@mc-review/hpp": "workspace:*",
        "@mc-review/helpers": "workspace:*",
        "@apollo/client": "3.8.8",
        "@apollo/gateway": "^2.8.5",
        "@aws-sdk/client-amplify": "^3.734.0",
        "@aws-sdk/client-cognito-identity-provider": "^3.734.0",
        "@aws-sdk/client-lambda": "^3.734.0",
        "@aws-sdk/client-rds": "^3.734.0",
        "@aws-sdk/client-s3": "^3.734.0",
        "@aws-sdk/client-secrets-manager": "^3.734.0",
        "@aws-sdk/client-ses": "^3.734.0",
        "@aws-sdk/client-ssm": "^3.734.0",
        "@aws-sdk/lib-storage": "^3.734.0",
        "@aws-sdk/s3-request-presigner": "^3.734.0",
        "@launchdarkly/node-server-sdk": "9.7.2",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/core": "^1.30.1",
        "@opentelemetry/exporter-trace-otlp-http": "^0.45.1",
        "@opentelemetry/id-generator-aws-xray": "^1.2.1",
        "@opentelemetry/propagator-aws-xray": "^1.26.0",
        "@opentelemetry/resources": "^1.18.1",
        "@opentelemetry/sdk-trace-base": "^1.18.1",
        "@opentelemetry/sdk-trace-node": "^1.18.1",
        "@opentelemetry/semantic-conventions": "^1.28.0",
        "@prisma/client": "^6.2.1",
        "apollo-server-core": "^3.11.1",
        "apollo-server-lambda": "^3.5.0",
        "apollo-server-types": "^3.8.0",
        "archiver": "^7.0.1",
        "axios": "^1.7.9",
        "eta": "^2.0.0",
        "graphql": "^16.9.0",
        "graphql-scalars": "^1.11.1",
        "jsonwebtoken": "^9.0.2",
        "neverthrow": "^6.0.0",
        "path-to-regexp": "^8.2.0",
        "prisma": "^5.17.0",
        "protobufjs": "^7.3.0",
        "purify-ts": "^2.0.1",
        "url-parse": "^1.5.10",
        "uuid": "^9.0.0",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@graphql-tools/jest-transform": "^2.0.0",
        "@luckycatfactory/esbuild-graphql-loader": "^3.8.1",
        "@types/archiver": "^6.0.3",
        "@types/aws-lambda": "^8.10.145",
        "@types/glob": "^8.0.0",
        "@types/jest": "^29.5.14",
